<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/corvinalbtn"
        android:layout_width="108dp"
        android:layout_height="133dp"
        android:layout_marginTop="175dp"
        android:onClick="openCorvinal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.198"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/botao" />

    <ImageButton
        android:id="@+id/grifibtn"
        android:layout_width="108dp"
        android:layout_height="133dp"
        android:layout_marginTop="189dp"
        android:onClick="openGrifinoria"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.811"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sonsebtn"
        app:srcCompat="@drawable/botao" />

    <ImageButton
        android:id="@+id/sonsebtn"
        android:layout_width="108dp"
        android:layout_height="133dp"
        android:layout_marginTop="175dp"
        android:onClick="openSonserina"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.811"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/botao" />

    <ImageButton
        android:id="@+id/lufabtn"
        android:layout_width="108dp"
        android:layout_height="133dp"
        android:layout_marginTop="189dp"
        android:onClick="openLufa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.198"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/corvinalbtn"
        app:srcCompat="@drawable/botao" />


</androidx.constraintlayout.widget.ConstraintLayout>

